{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "camelcase": ["error"],
        "prefer-const": ["error"],
        "no-delete-var": ["error"],
        "no-unused-vars": ["error"]
    }
}
